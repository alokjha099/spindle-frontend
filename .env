
# Backend API Base URL
BASE_URL=http://localhost:7777

# Razorpay Payment Gateway Credentials
RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_SECRET=your_razorpay_secret

# Socket.io Path (if needed)
